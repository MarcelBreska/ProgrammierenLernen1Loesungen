func turnAround() {
  turnLeft()
  turnLeft()
  moveForward()
  moveForward()
}

func solveRow() {
  turnRight()
  moveForward()
  collectGem()
  moveForward()
  collectGem()
  turnAround()
  moveForward()
  toggleSwitch()
  moveForward()
  toggleSwitch()
  turnAround()
  turnLeft()
  moveForward()
}

for i in 1...3 {
  solveRow()
}
